<br /><br />

<h1>List of users</h1>
<%= link_to 'create', new_admin_user_path %> <br />
<table>
  <tr>
      <th>unique name</th>
      <th>super admin?</th>
      <th>disabled?</th>
      <th>actions</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= h(user.unique_name) %></td>
      <td><%= user.super_admin? -%></td>
      <td><%= user.disabled? -%></td>
      <td><%= link_to 'edit', edit_admin_user_path(user), :id => "edit_#{h(user.unique_name)}" %>,
          <%= link_to 'delete', admin_user_path(user), :id => "delete_#{h(user.unique_name)}", :method => :delete %></td>
    </tr>
  <% end %>
</table>
